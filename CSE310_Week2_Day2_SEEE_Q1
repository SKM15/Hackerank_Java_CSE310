import java.util.Scanner;
class Solution
{
public static void main(String...args)
{
Scanner obj = new Scanner(System.in);
    int A = obj.nextInt();
    
    if (A>0)
    {
    double B = (double)A;
    System.out.println(B);
    }
    else
    {
            System.out.println("Invalid Input");     
    }
}
}
