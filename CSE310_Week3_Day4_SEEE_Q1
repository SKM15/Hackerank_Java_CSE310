import java.util.Scanner;

public class Solution 
{

    public static void main(String...args) 
    {
        Scanner obj = new Scanner(System.in);
        int rows=obj.nextInt();
        for(int i=rows;i>=1;i--) 
        {
            for(int j=1;j<=i;j++)
            {
                System.out.print(j+" ");
            }
             
            System.out.println();
        }
        
    }
}
