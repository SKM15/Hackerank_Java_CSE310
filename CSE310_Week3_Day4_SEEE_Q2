import java.util.Scanner;
 
public class Solution 
{ 
 
    public static void main(String...args) 
    { 
        Scanner obj = new Scanner(System.in); 
        double borrowedAmount = obj.nextDouble(); 
        double interest=0.10*borrowedAmount; 
        double totalAmount=borrowedAmount+interest; 
        int months=12; 
        double EMI = totalAmount /months; 
        System.out.println((int)totalAmount); 
        System.out.printf("%.2f",EMI); 
         
    } 
}
