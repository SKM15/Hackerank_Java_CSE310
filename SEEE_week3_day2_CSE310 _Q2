import java.util.Scanner;

public class Solution {

    public static void main(String...args) {
               Scanner scan = new Scanner(System.in);
        int num = scan.nextInt();
        if (num<0)
        {
            System.out.println("Invalid Input");
        }
        else if (num==1)
        {
            System.out.println("&");
        } else if (num==2) {
            System.out.println("&");
            System.out.println("&&");
        }
    }
       
    }
