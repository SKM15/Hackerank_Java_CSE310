import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) 
    
    {
         Scanner obj = new Scanner(System.in);
        int N = obj.nextInt();
        int cube;
        
        for (int i=0;i<=N;i++) 
         {
            cube = i * i * i;
            if (cube == N)
            {
                System.out.println("Perfect Cube");
                return;
            } 
            else if (cube > N) 
            {
                System.out.println("Not Perfect Cube");
               return;
            }
        }
    }
}
