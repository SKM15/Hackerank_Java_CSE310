
import java.util.Scanner;

public class Solution {

    public static void main(String...args) {
          Scanner obj = new Scanner(System.in);
        int i,fact=1;
        int number = obj.nextInt();
        for(i=1;i<=number;i++){
            fact=fact*i;
        }
        System.out.println(fact);
        
    }
}
